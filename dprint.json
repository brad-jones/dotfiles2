{
  "incremental": true,
  "typescript": {
    "deno": true,
    "module.sortImportDeclarations": "caseInsensitive",
    "module.sortExportDeclarations": "caseInsensitive"
  },
  "json": {},
  "markdown": {},
  "dockerfile": {},
  "prettier": {
    "associations": [
      "**/*.{yml,yaml}"
    ]
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,dockerfile,yml,yaml}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.79.0.wasm",
    "https://plugins.dprint.dev/json-0.17.0.wasm",
    "https://plugins.dprint.dev/markdown-0.15.1.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.0.wasm",
    "https://plugins.dprint.dev/prettier-0.17.0.json@5e42cbabab9906ad32ffe1aa67f15824b1c39408840f0afeaddc495dd362e177"
  ]
}
